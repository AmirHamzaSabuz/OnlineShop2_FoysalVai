@*
@using OnlineShop2.Models
@model IEnumerable<ProductType>
*@

@model IEnumerable<OnlineShop2.Models.ProductType>


<div class="row">
    <div class="col-10 offset-1">


    <div class="row">
        <div class="col-6">
            <h2 class="text-info">Product Type List</h2>
        </div>
        <div class="col-6 text-right">
                <a asp-action="Create" class="btn btn-info float-end"> &nbsp; New Product Type &nbsp; </a>
        </div>
    </div>
    <br/>
    <div>
        <table class="table table-striped border">
            <tr class="table-info">
                <th>
                    @Html.DisplayNameFor(model => model.ProductTypeName)
                </th>
                <th></th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.ProductTypeName</td>
                    <td>Edit | Details | Delete</td>
                </tr>
            }
            <tr>
            </tr>
        </table>
    </div>
    </div>
</div>
